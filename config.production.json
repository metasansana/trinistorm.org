{
    "storage": {
        "active": "ghost-storage-cloudinary",
        "ghost-storage-cloudinary": {
            "useDatedFolder": false,
            "upload": {
                "use_filename": true,
                "unique_filename": false,
                "overwrite": false,
                "folder": "trinistorm.org",
                "tags": ["blog"]
            },
            "fetch": {
                "quality": "auto",
                "secure": true,
                "cdn_subdomain": true
            }
        }
    }

}
